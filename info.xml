<?xml version="1.0"?>
<extension key="mtms" type="module">
  <file>mtms</file>
  <name>mtms</name>
  <description>Send and receive text messages and SMS donations in CiviCRM using mtms.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Figel</author>
    <email>pfigel@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/mtms</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/mtms</url>
    <url desc="Support">https://github.com/greenpeace-cee/mtms/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-09-01</releaseDate>
  <version>1.0-alpha.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Mtms</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
